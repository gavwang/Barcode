<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="root" prefHeight="108.0" prefWidth="264.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.wingain.controller.PasswordAuthDialog">
   <children>
      <Label layoutX="14.0" layoutY="14.0" prefHeight="29.0" prefWidth="90.0" text="%password">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <PasswordField fx:id="password" layoutX="104.0" layoutY="15.0" onKeyPressed="#onPasswordInputDone" prefHeight="27.0" prefWidth="150.0" />
      <Button layoutX="104.0" layoutY="65.0" mnemonicParsing="false" onAction="#onPasswordOK" prefHeight="27.0" prefWidth="69.0" text="%ok" />
      <Button layoutX="14.0" layoutY="65.0" mnemonicParsing="false" onAction="#onPasswordCancle" text="%cancle" />
      <Button layoutX="188.0" layoutY="65.0" mnemonicParsing="false" onAction="#onPasswordReset" prefHeight="27.0" prefWidth="69.0" text="%reset" />
      <Label fx:id="logininfo" layoutX="14.0" layoutY="46.0" prefHeight="17.0" prefWidth="238.0" textFill="RED">
         <font>
            <Font size="10.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
